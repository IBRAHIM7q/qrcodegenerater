{
  "name": "qr-code-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "scripts": {
    "dev": "vite",
    "build": "npx prisma generate --schema=./prisma/schema.prisma && vite build && npm run build:functions",
    "build:functions": "netlify-lambda build netlify/functions --config ./netlify-lambda-webpack.config.cjs",
    "preview": "vite preview",
    "server": "node server.js",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "express": "^4.21.2",
    "firebase-admin": "^11.11.0",
    "formidable": "^3.5.1",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "jspdf": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "netlify-lambda": "^2.0.16",
    "next": "^14.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qr-code-styling": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/express": "^4.17.23",
    "@types/formidable": "^2.0.6",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.14.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-loader": "^10.0.0",
    "prisma": "^5.22.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "webpack": "^5.102.1"
  }
}
